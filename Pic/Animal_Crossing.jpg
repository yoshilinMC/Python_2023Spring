function reviseFavoriteClass(page = 0) {
  let save = '收藏文章',saved = '取消收藏文章';
  let status = $('.function__save.desktop').eq(page).data("status");
  if (status) {
      $('.function__save.desktop').eq(page).attr('data-tooltips', saved);
      $('.function__save.desktop').eq(page).children().children('i').addClass('icon-bookmark-filled');
      $('.function__save.mobile').eq(page).attr('data-tooltips', saved);
      $('.function__save.mobile').eq(page).children().children('i').addClass('icon-bookmark-filled');
  } else {
      $('.function__save.desktop').eq(page).attr('data-tooltips', save);
      $('.function__save.desktop').eq(page).children().children('i').addClass('icon-bookmark-outlined');
      $('.function__save.mobile').eq(page).attr('data-tooltips', save);
      $('.function__save.mobile').eq(page).children().children('i').addClass('icon-bookmark-outlined');
  }
}

$(function () {
  var AppierArticle = Appier.article(appier_aiqua_params);
  let save = '收藏文章',saved = '取消收藏文章';
  reviseFavoriteClass(0)

  $('main').on("click", ".function__save", function() {
          window.location.replace("https://www.cw.com.tw/member/login");
      return;
    
    if ($(this).data("status")) {
      $.ajax({
        url: 'https://www.cw.com.tw/article/unCollect',
        context: this,
        data: {article_id: $(this).attr('data-id')},
        type: "GET",
        success: function (json) {
          items = json.items;
          if (items == null) {
            alert("取消收藏發生問題");
          }
        }
      }).then(function (data) {
        if (data.code == "US007") {
          $('#trigger-email-verification').trigger('click');
          return;
        }
        $(this).attr('data-tooltips', save);
        $(this).children().children('i').removeClass('icon-bookmark-filled').addClass('icon-bookmark-outlined');
        $('#article__unsave').fadeIn();
        $(this).data("status", false);
        
        AppierArticle.bookmark(false);
        setTimeout(function () {
          $('.message--notification').fadeOut();
        }, 2000);
      });
    } else {
      $.ajax({
        url: 'https://www.cw.com.tw/article/collect',
        context: this,
        data: {article_id: $(this).attr('data-id')},
        type: "GET",
        success: function (json) {
          items = json.items;
          if (items == null) {
            alert("收藏發生問題");
          }
        }
      }).then(function (data) {
        if (data.code == "US007") {
          $('#trigger-email-verification').trigger('click');
          return;
        }
        $(this).attr('data-tooltips', saved);
        $(this).children().children('i').removeClass('icon-bookmark-outlined').addClass('icon-bookmark-filled');
        $('#article__save').fadeIn();
        $(this).data("status", true);
        
        AppierArticle.bookmark(true);
        setTimeout(function () {
          $('.message--notification').fadeOut();
        }, 2000);
      });
    }
  });
});

